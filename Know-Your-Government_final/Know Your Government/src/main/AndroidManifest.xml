<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MAD.know_Your_Government">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.MAD.know_Your_Government.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.MAD.know_Your_Government.AboutActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName="com.example.MAD.know_Your_Government.MainActivity" >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.MAD.know_Your_Government.MainActivity" />
        </activity>


        <activity
            android:name="com.example.MAD.know_Your_Government.OfficialActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.MAD.know_Your_Government.MainActivity" >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.MAD.know_Your_Government.MainActivity" />
        </activity>


        <activity
            android:name="com.example.MAD.know_Your_Government.PhotoActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.MAD.know_Your_Government.OfficialActivity" >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.MAD.know_Your_Government.OfficialActivity" />
        </activity>


    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

</manifest>